package 经典问题;

public class 无重复数字问题 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum = 0;
		for(int a = 1;a<5;a++) {
			for(int b = 1;b<5;b++) {
				for(int c = 1;c<5;c++) {
					if(a!=b&&b!=c&&a!=b) {
						System.out.print(a*100+b*10+c+"\t");
						sum++;
						if(sum==5) {
							System.out.println();
							sum=0;
						}
					}
				}
			}
		}
		System.out.println();
		System.out.print(sum);
	}

}
